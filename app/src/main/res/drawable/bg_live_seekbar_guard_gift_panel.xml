<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <solid android:color="#33cecece"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="10dp"/>
                <gradient android:startColor="@color/live_gift_panel_seek_bar_guard_secondary_start_color" android:endColor="@color/live_gift_panel_seek_bar_guard_secondary_end_color" android:angle="0" android:centerColor="@color/live_gift_panel_seek_bar_guard_secondary_center_color"/>
            </shape>
        </scale>
    </item>
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="10dp"/>
                <gradient android:startColor="@color/live_gift_panel_seek_bar_guard_first_start_color" android:endColor="@color/live_gift_panel_seek_bar_guard_first_end_color" android:angle="0" android:centerColor="@color/live_gift_panel_seek_bar_guard_first_center_color"/>
            </shape>
        </scale>
    </item>
</layer-list>
