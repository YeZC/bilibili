<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/outer_drawable">
        <shape android:layout_width="wrap_content" android:layout_height="wrap_content" android:shape="oval">
            <stroke android:width="2dp" android:color="@color/Pi5"/>
        </shape>
    </item>
    <item android:id="@+id/inner_drawable" android:left="2dp" android:top="2dp" android:right="2dp" android:bottom="2dp">
        <shape android:layout_width="wrap_content" android:layout_height="wrap_content" android:shape="oval">
            <stroke android:width="2dp" android:color="@color/Wh0"/>
        </shape>
    </item>
</layer-list>
