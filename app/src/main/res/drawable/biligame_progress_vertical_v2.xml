<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="1.33dp"/>
            <solid android:color="@color/black_alpha30"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="1.33dp"/>
                <solid android:color="@color/Lb3_u"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip android:gravity="top" android:clipOrientation="vertical">
            <shape>
                <corners android:radius="1.33dp"/>
                <solid android:color="@color/Lb3_u"/>
            </shape>
        </clip>
    </item>
</layer-list>
