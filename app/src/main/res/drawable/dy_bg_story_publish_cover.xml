<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:layout_width="match_parent" android:layout_height="match_parent" android:shape="rectangle">
            <solid android:color="@color/Bg2"/>
            <corners android:radius="4dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="4dp"/>
            <stroke android:width="1dp" android:color="@color/Ga2" android:dashWidth="3dp" android:dashGap="3dp"/>
        </shape>
    </item>
</layer-list>
