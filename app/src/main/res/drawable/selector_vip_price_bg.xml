<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <corners android:radius="@dimen/corner_radius_medium"/>
            <stroke android:width="1dp" android:color="@color/Pi5"/>
            <solid android:color="@color/vip_light_pink"/>
        </shape>
    </item>
    <item android:state_selected="false">
        <shape>
            <corners android:radius="@dimen/corner_radius_medium"/>
            <stroke android:width="@dimen/divider_line_height" android:color="@color/Ga4"/>
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
</selector>
