<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <solid android:color="@color/theme_color_live_bg_white_dark"/>
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/theme_color_live_bg_white"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/theme_color_live_bg_white" android:alpha="0.3"/>
        </shape>
    </item>
</selector>
