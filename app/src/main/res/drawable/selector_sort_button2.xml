<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <stroke android:width="0.5dp" android:color="@color/theme_color_secondary"/>
            <corners android:radius="12dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="0.5dp" android:color="@color/theme_color_live_text_assist_dark"/>
            <corners android:radius="12dp"/>
        </shape>
    </item>
</selector>
