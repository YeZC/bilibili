<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="32dp"/>
            <solid android:color="@color/Ga2"/>
        </shape>
    </item>
    <item android:state_enabled="true" android:state_selected="false">
        <shape>
            <corners android:radius="32dp"/>
            <stroke android:width="1px" android:color="@color/Ga4"/>
            <solid android:color="@color/Wh0"/>
        </shape>
    </item>
    <item android:state_enabled="true" android:state_selected="true">
        <shape>
            <corners android:radius="32dp"/>
            <stroke android:width="1px" android:color="@color/Pi5"/>
            <solid android:color="@color/Wh0"/>
        </shape>
    </item>
</selector>
